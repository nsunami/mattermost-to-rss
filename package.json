{
	"name": "mattermost-rss-api",
	"version": "1.0.0",
	"description": "REST API that converts messages mentioning a Mattermost bot to RSS feed",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"test": "jest",
		"lint": "eslint .",
		"docker:build": "docker build -t mattermost-rss-api .",
		"docker:run": "docker run -p 3000:3000 --env-file .env mattermost-rss-api"
	},
	"keywords": ["mattermost", "rss", "api", "bot", "feed"],
	"author": "Nami Sunami",
	"license": "MIT",
	"dependencies": {
		"express": "^4.18.2",
		"axios": "^1.6.0",
		"rss": "^1.2.2",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1"
	},
	"devDependencies": {
		"nodemon": "^3.0.1",
		"jest": "^29.7.0",
		"eslint": "^8.52.0",
		"supertest": "^6.3.3"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}

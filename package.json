{
  "name": "mattermost-rss-api",
  "version": "1.0.0",
  "description": "REST API that converts messages mentioning a Mattermost bot to RSS feed",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "lint": "eslint .",
    "docker:build": "docker build -t mattermost-rss-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mattermost-rss-api"
  },
  "keywords": [
    "mattermost",
    "rss",
    "api",
    "bot",
    "feed"
  ],
  "author": "Nami Sunami",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "feed": "^5.1.0",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/rss": "^0.0.32",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
